<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="537.0" prefWidth="867.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group5.navana3s_workshop.Shanjana.ServiceHistoryController">
    <children>
        <Text layoutX="45.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Service History">
            <font>
                <Font name="Arial Bold Italic" size="23.0" />
            </font>
        </Text>

        <TableView fx:id="serviceHistoryTable" layoutX="50.0" layoutY="65.0" prefHeight="284.0" prefWidth="766.0">
            <columns>
                <TableColumn fx:id="bookingIdCol" prefWidth="120.0" text="Booking ID" />
                <TableColumn fx:id="dateCol" prefWidth="140.0" text="Service Date" />
                <TableColumn fx:id="serviceTypeCol" prefWidth="180.0" text="Service Type" />
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>

        <Button layoutX="757.0" layoutY="454.0" mnemonicParsing="false" onAction="#BackButton" prefHeight="26.0" prefWidth="58.0" style="-fx-background-color: #3e3e3e;" text="Back" textFill="WHITE" />
      <Button layoutX="732.0" layoutY="371.0" mnemonicParsing="false" onAction="#LoadHistory" style="-fx-background-color: #3e3e3e;" text="Load History" textFill="WHITE" />
    </children>
</AnchorPane>
